  "components": {
    "securitySchemes": {
      "ApiKeyAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "X-API-Key"
      },
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer"
      }
    },
    "schemas": {
      "Error": {
        "type": "object",
        "properties": {
          "error": {
            "type": "string",
            "description": "Error message"
          },
          "code": {
            "type": "integer",
            "description": "Error code"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "HealthCheck": {
        "type": "object",
        "properties": {
          "agent_id": {
            "type": "string",
            "description": "Unique agent identifier"
          },
          "agent_name": {
            "type": "string",
            "description": "Agent name"
          },
          "status": {
            "type": "string",
            "enum": [
              "healthy",
              "unhealthy",
              "a2a_healthy_api_unhealthy"
            ],
            "description": "Overall health status"
          },
          "health_score": {
            "type": "number",
            "minimum": 0.0,
            "maximum": 1.0,
            "description": "Health score (0.0-1.0)"
          },
          "a2a_connected": {
            "type": "boolean",
            "description": "A2A protocol connection status"
          },
          "api_accessible": {
            "type": "boolean",
            "description": "External API accessibility"
          },
          "data_domain": {
            "type": "string",
            "description": "Data domain"
          },
          "capabilities": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Available capabilities"
          },
          "performance_metrics": {
            "type": "object",
            "properties": {
              "total_requests": {
                "type": "integer",
                "description": "Total requests processed"
              },
              "error_count": {
                "type": "integer",
                "description": "Total errors"
              },
              "error_rate": {
                "type": "number",
                "description": "Error rate (0.0-1.0)"
              },
              "avg_response_time": {
                "type": "number",
                "description": "Average response time in seconds"
              }
            }
          }
        }
      }
    }
  }
}