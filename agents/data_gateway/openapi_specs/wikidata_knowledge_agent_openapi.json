      }
    },
    "/metrics": {
      "get": {
        "summary": "Get WIKIDATA_KNOWLEDGE_AGENT Prometheus metrics",
        "description": "Retrieve Prometheus-formatted metrics for monitoring the WIKIDATA_KNOWLEDGE_AGENT agent performance and health.",
        "operationId": "getMetrics",
        "tags": [
          "Monitoring"
        ],
        "responses": {
          "200": {
            "description": "Metrics retrieved successfully",
            "content": {
              "text/plain": {
