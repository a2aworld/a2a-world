    "/health": {
      "get": {
        "summary": "Get WIKIDATA_KNOWLEDGE_AGENT health status",
        "description": "Retrieve comprehensive health information for the WIKIDATA_KNOWLEDGE_AGENT agent, including connection status, performance metrics, and external API accessibility.",
        "operationId": "getHealth",
        "tags": [
          "Health"
        ],
        "responses": {
          "200": {
            "description": "Health check successful",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HealthCheck"
                }
              }
            }
          },
          "503": {
